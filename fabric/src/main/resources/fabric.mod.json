{
  "schemaVersion": 1,
  "id": "hexconjuring",
  "version": "${version}",
  "name": "Hex Conjuring",
  "description": "A Hex Casting addon which allows you to conjure objects and tools",
  "authors": [
    "demonpie",
    "Luckly",
    "Endaboi"
  ],
  "contact": {
    "homepage": "https://demonpie.github.io/hexconjuring",
    "sources": "https://github.com/demonpie/hexconjuring"
  },
  "license": "MIT",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.hexconjuring.fabric.HexconjuringFabric"
    ],
    "client": [
      "net.hexconjuring.fabric.HexconjuringClientFabric"
    ]
  },
  "mixins": [
    "hexconjuring-common.mixins.json",
    "hexconjuring.mixins.json"
  ],
  "depends": {
    "minecraft": ">=${minecraftVersion}",
    "fabricloader": ">=${fabricLoaderVersion}",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=${fabricKotlinVersion}",
    "architectury": ">=${architecturyVersion}",
    "hexcasting": ">=${hexcastingVersion}"
  }
}
